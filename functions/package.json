{
  "name": "mono-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "predist:get": "rm -f ./main.js ./main.js.map ./*.txt; rm -rf ./proto",
    "dist:get": "cp -r ../dist/apps/api/* ./; cp -r ../tools/proto ./proto",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.5.6",
    "@nestjs/common": "7.6.14",
    "@nestjs/core": "7.6.14",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/microservices": "7.6.14",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.6.14",
    "@nestjs/platform-ws": "7.6.14",
    "@nestjs/websockets": "7.6.14",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "firebase-admin": "9.5.0",
    "firebase-functions": "3.13.2",
    "google-protobuf": "3.15.6",
    "grpc": "1.24.6",
    "grpc-web": "1.2.1",
    "nodemailer": "6.5.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.6"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
